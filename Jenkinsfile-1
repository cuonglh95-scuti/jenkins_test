pipeline {
    agent {
        docker { image 'node:16-alpine' }
    }

    stages {
        stage('Clone') {
            steps {
                git branch: 'develop-2', url: 'https://github.com/cuonglh95-scuti/jenkins_test.git'
            }
        }

        stage('Test') {
            steps {
                sh 'node --version'
            }
        }
    }
}
